{
  "backend_strategy": "sqlite",
  "listener_sink_type": "sqlite",
  "ingest_sink_type": "asyncsqlite",

  "backend_endpoint": "http://localhost:8000",
  "sqlite_path": "eventlog.db",

  "viewer_backend_host": "127.0.0.1",
  "viewer_backend_port": 8000,
  "ingest_backend_host": "127.0.0.1",
  "ingest_backend_port": 8001,

  "loki_endpoint": "http://localhost:3100",

  "source_log_type": "file", 
  "source_log_path": "../flask-logging-demo/single_file_app_pattern/app.log",
  "source_log_label": "flask-app",
  "log_tail_poll_interval": 1.0,

  "source_log_tails": [
    {
      "log_path": "../flask-logging-demo/single_file_app_pattern/app.log",
      "label": "flask-app",
      "poll_interval": 1.0,
      "event_type": "app_log",
      "log_level": "INFO"
    },
    {
      "path": "../flask-logging-demo/single_file_app_pattern/www.log",
      "label": "flask-www",
      "poll_interval": 1.0,
      "event_type": "www_log",
      "log_level": "INFO"
    }
  ],
  "log_level": "INFO",
  "log_level_listener": "",
  "log_level_backend": "",
  "log_level_cli": ""
  }